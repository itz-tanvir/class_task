#include<bits/stdc++.h>
using namespace std;

int find_occuarence_of_k(int n, int arr[],int k)
{
   int cnt=0;
   for(int  i=0;i<n;++i){
      if(k==arr[i]){
        cnt++;
      }
   }
   return cnt;
}
int main()
{
  int n,k;
  cin >> n >> k;
  int arr[n+20];
  for(int i=0;i<n;++i){
    cin >> arr[i];
  }
  cout << find_occuarence_of_k(n,arr,k) << endl;
}
