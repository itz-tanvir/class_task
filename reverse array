#include<bits/stdc++.h>
using namespace std;

void reverse_array(int n, int arr[])
{
   int brr[n+20];
   for(int i=0;i<n;++i){
     brr[i]=arr[n-i-1];
   }
   for(int i=0;i<n;++i){
     cout << brr[i] << endl;
   }
}
int main()
{
  int n;
  cin >> n ;
  int arr[n+20];
  for(int i=0;i<n;++i){
    cin >> arr[i];
  }
  reverse_array(n,arr);
}
